{
    "name": "taylornetwork/setting",
    "description": "A Laravel setting manager per user",
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.*.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "TaylorNetwork\\Setting\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaylorNetwork\\Setting\\SettingServiceProvider"
            ],
            "aliases": {
                "Setting": "TaylorNetwork\\Setting\\Facades\\Setting"
            }
        }
    }
}
